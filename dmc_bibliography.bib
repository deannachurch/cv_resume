@ARTICLE{Patwardhan2015-pf,
  title     = "Achieving high-sensitivity for clinical applications using
               augmented exome sequencing",
  author    = "Patwardhan, Anil and Harris, Jason and Leng, Nan and Bartha,
               Gabor and Church, Deanna M and Luo, Shujun and Haudenschild,
               Christian and Pratt, Mark and Zook, Justin and Salit, Marc and
